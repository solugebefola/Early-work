<h1>Sign up to RedClone</h1>

<form action="<%= users_url %>" method="POST">
<%= auth_token %>

  <label>Email
    <input
      type="text"
      name="user[email]"
      id="user_email">
  </label>
  
  <label>Password
    <input
      type="text"
      name="user[password]"
      id="user_password">
  </label>

  <input type="submit" value="Sign up">
</form>
